{
    "@context": [
        "https://www.w3.org/2019/wot/td/v1",
        {
            "@language": "en"
        }
    ],
    "@type": "Thing",
    "id": "de:tum:ei:esi:fp:coffee:1-1",
    "title": "Virtual-Coffee-Machine_1_1",
    "description": "A virtual coffee machine to learn the WoT TD standard",
    "security": "nosec_sc",
    "securityDefinitions": {
        "nosec_sc": {
            "scheme": "nosec"
        }
    },
    "properties": {
        "state": {
            "type": "string",
            "readOnly": true,
            "enum": [
                "Ready",
                "Brewing",
                "Error"
            ],
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/properties/state",
                    "contentType": "application/json",
                    "op": [
                        "readproperty"
                    ],
                    "htv:methodName": "GET"
                }
            ],
            "writeOnly": false,
            "observable": false
        },
        "waterStatus": {
            "type": "integer",
            "readOnly": true,
            "unit": "%",
            "maximum": 100,
            "minimum": 0,
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/properties/waterStatus",
                    "contentType": "application/json",
                    "op": [
                        "readproperty"
                    ],
                    "htv:methodName": "GET"
                }
            ],
            "writeOnly": false,
            "observable": false
        },
        "coffeeStatus": {
            "type": "integer",
            "readOnly": true,
            "unit": "%",
            "maximum": 100,
            "minimum": 0,
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/properties/coffeeStatus",
                    "contentType": "application/json",
                    "op": [
                        "readproperty"
                    ],
                    "htv:methodName": "GET"
                }
            ],
            "writeOnly": false,
            "observable": false
        },
        "binStatus": {
            "type": "integer",
            "readOnly": true,
            "unit": "%",
            "maximum": 100,
            "minimum": 0,
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/properties/binStatus",
                    "contentType": "application/json",
                    "op": [
                        "readproperty"
                    ],
                    "htv:methodName": "GET"
                }
            ],
            "writeOnly": false,
            "observable": false
        }
    },
    "actions": {
        "brew": {
            "input": {
                "type": "string",
                "enum": [
                    "latte-machiato",
                    "espresso",
                    "cappuccino"
                ]
            },
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/actions/brew",
                    "contentType": "application/json",
                    "op": [
                        "invokeaction"
                    ],
                    "htv:methodName": "POST"
                }
            ],
            "idempotent": false,
            "safe": false
        },
        "abort": {
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/actions/abort",
                    "contentType": "application/json",
                    "op": [
                        "invokeaction"
                    ],
                    "htv:methodName": "POST"
                }
            ],
            "idempotent": false,
            "safe": false
        },
        "shutdown": {
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/actions/shutdown",
                    "contentType": "application/json",
                    "op": [
                        "invokeaction"
                    ],
                    "htv:methodName": "POST"
                }
            ],
            "idempotent": false,
            "safe": false
        }
    },
    "events": {
        "maintenance": {
            "data": {
                "type": "string"
            },
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/events/maintenance",
                    "contentType": "application/json",
                    "subprotocol": "longpoll",
                    "op": [
                        "subscribeevent"
                    ]
                }
            ]
        },
        "error": {
            "data": {
                "type": "string"
            },
            "forms": [
                {
                    "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/events/error",
                    "contentType": "application/json",
                    "subprotocol": "longpoll",
                    "op": [
                        "subscribeevent"
                    ]
                }
            ]
        }
    },
    "forms": [
        {
            "href": "http://esiremotelab.esi.ei.tum.de:8080/Virtual-Coffee-Machine_1_1/all/properties",
            "contentType": "application/json",
            "op": [
                "readallproperties",
                "readmultipleproperties",
                "writeallproperties",
                "writemultipleproperties"
            ]
        }
    ]
}